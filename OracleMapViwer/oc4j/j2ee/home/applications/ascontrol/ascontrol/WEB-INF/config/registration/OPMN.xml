    <property name="COMPONENT_TYPE" value="OPMN"/>
    <property name="COMPONENT_NAME" value='ID_VLOGS_PROCESS_MGR@oracle.sysman.ias.studio.logging.msg.LoggingMsg'/>
    <property name="OPMN_STDOUT_STDERR" value='ID_VLOGS_OPMN_STDOUT@oracle.sysman.ias.studio.logging.msg.LoggingMsg'/>
