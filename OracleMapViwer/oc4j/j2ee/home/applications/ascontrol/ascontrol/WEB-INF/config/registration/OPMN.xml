 <log path='opmn/logs/HTTP_Server~1.log' componentId='OHS'>
  <logreader class='oracle.core.ojdl.reader.UnformattedTextLogReaderFactory'>
    <property name='ComponentId' value='OHS'/>
    <property name='ModuleId' value='console'/>
  </logreader>
   <logviewer LogType='SERVER' ComponentType='HTTP Server'            
              ComponentName='ID_VLOGS_OPMN_STDOUT@oracle.sysman.ias.studio.logging.msg.LoggingMsg'>
    <property name='category' value='diagnostic'/>
  </logviewer>  
 </log>
 
