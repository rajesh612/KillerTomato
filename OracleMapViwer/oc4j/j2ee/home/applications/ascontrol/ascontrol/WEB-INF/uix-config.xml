<?xml version="1.0" encoding="ISO-8859-1"?>
  <configurations xmlns="http://xmlns.oracle.com/uix/config">

  <!-- if false, no tracing messages will be written -->
    <application-configuration debug="false">
      <debug-indent-output>true</debug-indent-output>
      <default-expression-language>data</default-expression-language>
      <ui-extensions>
        <extension-class>oracle.sysman.emSDK.chart.EmChartUIExtension</extension-class>
      </ui-extensions>
    </application-configuration>


  <!-- An alternate configuration that disables accessibility features  -->
  <default-configuration>
   <accessibility-mode>inaccessible</accessibility-mode>

   <supported-locales>
     <default-locale>en</default-locale>
     <supported-locale>de</supported-locale>
     <supported-locale>es</supported-locale>
     <supported-locale>fr</supported-locale>
     <supported-locale>it</supported-locale>
     <supported-locale>ja</supported-locale>
     <supported-locale>ko</supported-locale>
     <supported-locale>pt-BR</supported-locale>
     <supported-locale>zh-CN</supported-locale>
     <supported-locale>zh-TW</supported-locale>
   </supported-locales>

  </default-configuration>

  </configurations>
