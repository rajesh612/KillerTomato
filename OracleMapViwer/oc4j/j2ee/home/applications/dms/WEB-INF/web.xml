<?xml version="1.0" standalone='yes'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

  <servlet>
    <servlet-name>Spy</servlet-name>
    <servlet-class>Spy</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AggreSpy</servlet-name>
    <servlet-class>AggreSpy</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Spy</servlet-name>
    <url-pattern>/Spy</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AggreSpy</servlet-name>
    <url-pattern>/AggreSpy</url-pattern>
  </servlet-mapping>

</web-app>
